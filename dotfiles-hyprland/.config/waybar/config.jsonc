{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  "reload_style_on_change": true,
  // "width": 1280, // Waybar width
  // Choose the order of the modules
  "modules-left": [
    "hyprland/workspaces",
    "hyprland/windowcount",
    "hyprland/window"
  ],
  "modules-center": [],
  "modules-right": [
    "group/sysinfo",
    "group/user"
  ],
  "group/sysinfo": {
    "orientation": "horizontal",
    "modules": [
      //      "custom/music",
      //     "cava",
      "pulseaudio",
      "bluetooth",
      "battery",
      "clock"
    ]
  },
  "group/user": {
    "orientation": "horizontal",
    "modules": [
      "custom/lock",
      "custom/power"
    ]
  },
  "hyprland/workspaces": {
    "disable-scroll": true,
    "cursor": true,
    "sort-by-name": true,
    "format": "{icon}",
    "format-icons": {
      "default": "ÓÆ¥"
    }
  },
  "tray": {
    "icon-size": 21,
    "spacing": 10
  },
  "custom/music": {
    "format": "ÔÄÅ  {}",
    "escape": true,
    "interval": 5,
    "tooltip": false,
    "exec": "playerctl metadata --format='{{ title }}'",
    "on-click": "playerctl play-pause",
    "max-length": 50
  },
  "cava": {
    //"cava_config": "$XDG_CONFIG_HOME/waybar/cava.conf",
    "framerate": 30,
    "autosens": 1,
    //"sensitivity": 100,
    "bars": 14,
    "lower_cutoff_freq": 50,
    "higher_cutoff_freq": 10000,
    "method": "pipewire",
    "source": "auto",
    "stereo": true,
    "reverse": false,
    "bar_delimiter": 0,
    "monstercat": false,
    "waves": false,
    "noise_reduction": 0.77,
    "input_delay": 2,
    "hide_on_silence": true,
    "sleep_timer": 1,
    "format-icons": [
      "‚ñÅ",
      "‚ñÇ",
      "‚ñÉ",
      "‚ñÑ",
      "‚ñÖ",
      "‚ñÜ",
      "‚ñá",
      "‚ñà"
    ],
    "actions": {
      "on-click-right": "mode"
    }
  },
  "clock": {
    "timezone": "America/Chicago",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "Ó™∞ {:%d/%m/%Y}",
    "format": "ÔÄó {:%H:%M}",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    }
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{icon}",
    "format-icons": [
      "Óéç",
      "Óèì",
      "Óèë",
      "Óèè",
      "Óèé",
      "Óèç",
      "Óèä",
      "Óèà",
      "Óéõ"
    ]
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{icon}",
    "format-charging": "Û∞ÇÑ",
    "format-plugged": "Û±âù",
    "format-alt": "{icon}",
    "format-icons": [
      "Û∞ÇÉ",
      "Û∞Å∫",
      "Û∞Åª",
      "Û∞Åº",
      "Û∞ÅΩ",
      "Û∞Åæ",
      "Û∞Åø",
      "Û∞ÇÄ",
      "Û∞ÇÅ",
      "Û∞ÇÇ",
      "Û∞Åπ"
    ]
  },
  "pulseaudio": {
    // "scroll-step": 1, // %, can be a float
    "format": "{icon} {volume}%",
    "format-muted": "Ôë¶",
    "format-icons": {
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "on-click": "pavucontrol"
  },
  "custom/lock": {
    "tooltip": false,
    //"on-click": "sh -c '(sleep 0.5s; swaylock --grace 0)' & disown",
    "format": "ÔÄ£"
  },
  "custom/power": {
    "tooltip": false,
    //"on-click": "wlogout &",
    "format": "‚èª"
  }
}
